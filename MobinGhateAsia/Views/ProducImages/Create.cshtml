@model Models.ProducImage

 

<h2>افزودن @ViewBag.Title</h2>

@using (Html.BeginForm("Create", "ProducImages",new{ imageTypeId=ViewBag.ProductImageTypeId },FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    @Html.AntiForgeryToken()
    
    <div class="form-horizontal">        
        @Html.ValidationSummary(true)

        <div class="form-group">
        <div class="form-group">
            @Html.LabelFor(model => model.ImageUrl, new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                    @Html.Kendo().Upload().Name("fileUpload").Multiple(false)
                @Html.ValidationMessageFor(model => model.ImageUrl)
            </div>
        </div>

     

        <div class="form-group" id="buttons">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="ایجاد" class="btn btn-primary" />
				@Html.ActionLink("بازگشت به فهرست", "Index",new{id=ViewBag.ProductId, imageTypeId = ViewBag.ProductImageTypeId })
            </div>
        </div>
        </div>
</div>
}

